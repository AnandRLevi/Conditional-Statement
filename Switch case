#include <stdio.h>

int main()
{
    int ch;
    printf("Enter your number: ");
    scanf("%d",&ch);
    switch(ch)
    {
        case 1:
        printf("Item: Pizza\nPrice: Rs.179");
        break;
        case 2:
        printf("Item: Pasta\nPrice: Rs.179");
        break;
        case 3:
        printf("Item: Sandwich\nPrice: Rs.150");
        break;
        case 4:
        printf("Item: Burger\nPrice: Rs.130");
        break;
        case 5:
        printf("Item: French Fries\nPrice: Rs.70");
        break;
        default:
        printf("Invalid number");
        break;
    }
}
